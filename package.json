    "sharp": "^0.32.6",
    "typescript": "^4.5.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8",
    "vite-plugin-monkey": "^4.0.6",
    "vite-plugin-svgr": "^4.2.0",
    "yaml": "^1.10.0"
