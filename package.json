    "icon": "node scripts/icoFetcher.js",
    "support": "node scripts/supported.js",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 10"
  ],
  "browser": {
    "Buffer": false,
    "path": false
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": "npx eslint --max-warnings 0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
