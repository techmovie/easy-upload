  "author": "birdplane",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "start": "vite preview",
    "commit": "git add -A & git-cz",
    "release": "node scripts/release.js",
