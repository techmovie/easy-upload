    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "commitizen": "^4.3.1",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "decode-ico": "^0.4.1",
    "eslint": "^9.12.0",
    "eslint-config-preact": "^1.2.0",
    "eslint-config-standard": "^17.1.0",
